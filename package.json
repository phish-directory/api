    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "swagger-autogen": "^2.23.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
